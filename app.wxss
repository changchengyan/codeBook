/** 全局通用 app.wxss**/
view{box-sizing:border-box;}
scroll-view{box-sizing:border-box;}

/**正在装载列表信息**/
.list-loadding{ padding: 20px; text-align: center; text-align: center; color: #999;}
.list-loadding .icon image{ width:50px; height: 10px;filter:grayscale(100%); }
.list-loadding .text{ font-size: 12px; color: #999;}

/**没有列表信息**/
.nodata-canvas{ text-align: center; padding: 40px 0px 40px 0px;}
.nodata-canvas image{ width: 120px; height: 120px;}
.nodata-canvas .text{color: #999; padding: 10px 0px 0px 0px; line-height: 18px;}

/**没有网络**/
.no-connection{ padding: 20px; text-align: center; text-align: center; color: #999;}
.no-connection .icon image{ width:240px; height: 240px;filter:grayscale(100%); }
.no-connection .text{ font-size: 12px; color: #999;}

/***通用表单信息***/
.pub-submit{background-color: #06c1ae !important; border:1px hidden #03b5a3 !important; color: #fff !important; line-height:40px !important; font-size: 16px !important;}

.pub-to-canvas{ padding: 15px; background-color: #f8f8f8; text-align: center;}
.pub-to-home{background-color: #06c1ae !important; border:1px hidden #03b5a3 !important; color: #fff !important; line-height:40px !important; font-size: 16px !important; width: 60%; border-radius: 5px; text-align: center; margin: auto;}

/***通用导航图标***/
.pub-navigation{ 
    position: fixed; z-index: 100; bottom: 15px; left: 0px; width: 100%;
    display:none;
    flex-direction:row;
    justify-content:space-between;
    align-items:flex-start;
    padding: 0px 15px 0px 15px;
}
.pub-navigation image{ width: 35px; height: 35px;border-radius:35px;}
.pub-navigation .home{ display:none;width: 35px; height: 35px;background-color:#06c1ae; border-radius:35px; opacity:0.6;}
.pub-navigation .user{ display:none;width: 35px; height: 35px;background-color: #06c1ae; border-radius:35px;opacity:0.6;}
/**图片**/
image{
	height:100px;
}
/**购买模板**/
page {
  width: 100%;
  height: 100%; 
}
.template-black-mask {
  position: fixed;
  top: 0;
  left:0;
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.5;
  z-index: 1000;
}
.template-semicircle {
  position: absolute;
  top: -70rpx;
  left:0;
  right:0;
  margin:0 auto;
  padding-top: 22rpx;
  width: 137rpx;
  height: 137rpx;
  border-radius: 50%;
  background: #fff;
  text-align:center;
}
.template-semicircle image {
  width: 40rpx;
  height:16px;
}
.template-pay-box {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  position: fixed;
  left: 0;
  right: 0;
  top:50%;
  margin: -150px auto 0;
  z-index: 10000;
}
.template-pay-box .info-box {
  display: flex;
  width: 70%;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  border-radius: 30rpx;
  background: #fff;
  overflow: hidden
}
.template-pay-box .to-close {
 	width: 100%;
	padding:20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: 28rpx;
  line-height:60rpx;
}
.template-pay-box .to-close view{
	color:#999;
}


.to-close-box {
  width: 60rpx;
  height:60rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  
}
.to-close-box  image {
  width: 30rpx;
  height:13px;
  margin:auto 0;
}
.info-box .money {
  font-size: 50rpx;
  padding-bottom: 30rpx;
  color: #06c1ae;
}
.info-box .name {
  padding: 0 30rpx;
  font-size: 32rpx;
  color: #333;
}
.info-box .num {
  font-size: 20rpx;
  color: #999;
}
.info-box .loaddings {
  height:40px;
}
.to-pay-button {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  width: 100%;  
  height: 90rpx;
  background-color: #06c1ae;
  color: #fff;
  font-size: 36rpx;
  border-radius: 0;
  border-bottom-left-radius: 8rpx;
  border-bottom-right-radius: 8rpx;
}
/*实体商品*/
.seed-default-box {
    display: flex;
     flex-direction: column;
     justify-content: flex-start;
     align-items: center;
     position: fixed;
     width: 70%;
     left: 0;
     right: 0;
     top:50%;
     height: 474rpx;
     margin: -237rpx auto 0;
     z-index: 10000;
     background:#fff;
     border-radius: 4px;
    
}
.seed-default-semicircle {
 
  margin:-70rpx auto;
  padding-top: 22rpx;
  width: 137rpx;
  height: 137rpx;
  border-radius: 50%;
  background: #fff;
  text-align:center;
}
.buy-style {
  font-size: 26rpx;
}
.seed-default-semicircle image {
  width: 40rpx;
  height:16px;
}
.seed-default-head {
  width: 100%;
  display: flex;
  flex-direction:column;
  justify-content: flex-start;
  align-items:flex-start;
  padding: 10rpx 20rpx;
  border-bottom: 1px solid #ccc;
  margin-top: 24rpx;
}
.seed-default-box .to-close {
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  font-size: 23rpx;


}
.seed-default-box .to-close view{
	color:#999;
}
.seed-total-money {
   text-align: center;
   font-size: 50rpx;
   color: #06c1ae;
}
.seed-default-box form {
  display: flex;
  width: 100%;
  flex-direction: column;
  justify-content: flex-start;
   margin-top:20rpx;
}
.seed-info-word {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
}
.seed-name {
  margin: 10rpx 0;
  font-size: 24rpx;
}

.pay-info-item {
   display: flex;
   flex-direction: row;
  width: 100%;
  padding: 0 20rpx;
  font-size: 24rpx;
  height: 90rpx;
  border-bottom: 1px solid #eee;
  justify-content: flex-start;
  align-items: center;
}
.pay-info-title {
  margin:0;
  width: 124rpx;
  color: #333;
}
.pay-info-item input {
 width: 70%;
  border:0;
  padding-left:5rpx;
  border-radius:4px;
}
.address-textarea {
  width:70%;
  height: 40px;
  border:1px solid #ccc;
  padding-left:5rpx;
  border-radius:4px;
}
.pay-num {
  display: flex;
  flex-direction: row;
  justify-content: space-between; 
}
.change-num-box {
   display: flex;
  flex-direction: row;
  justify-content: space-between; 
}
.change-num {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 40rpx;
  height: 40rpx;
  border: 1px solid #06c1ae;
  border-radius: 50%;
  color: #06c1ae;
  font-size: 32rpx;
}
